	local function death()
		local randomPlayer = game.Players:GetPlayers()
		[math.random(1,#game.Players:GetPlayers())]
		playerRoot=randomPlayer.Character.HumanoidRootPart --:WaitForChild("HumanoidRootPart")

	end

