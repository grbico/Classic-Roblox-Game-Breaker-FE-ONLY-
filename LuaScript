force.Force=Vector3.new(0, 12544, 0)	
	mass=root.Shade.Torso:GetMass() force.Force=Vector3.new(0,mass*196,0)
	root.Shade.Humanoid.MaxHealth=999999999999999999999956987658786548754887349784397984378
	root.Shade.Humanoid.Health=999999999999999999999956987658786548754887349784397984378
